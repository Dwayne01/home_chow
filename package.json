{
  "name": "homechow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
		"build": "next build",
		"start": "next start -p 3003",
    "lint": "next lint",
		"lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
		"prettier:fix": "prettier --write '**/(*.js|.ts|.jsx|.tsx|.md)'",
		"prettier:check": "prettier --check '**/(*.js|.ts|.jsx|.tsx|.md)'",
		"coverage": "yarn test --coverage --watchAll --collectCoverageFrom=\"src/**/*.{ts,tsx,js,jsx}\"",
		"prepare": "husky install",
    "cypress:open": "cypress open",
		"cypress:run": "cypress run",
		"cypress:ci": "start-server-and-test 'yarn dev' :3003 'yarn cypress:run'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.7.6",
    "@apollo/server": "^4.3.2",
    "@next/font": "13.1.6",
    "@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "cypress": "^12.5.1",
    "autoprefixer": "^10.4.13",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-testing-library": "^5.10.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-helix": "^1.13.0",
    "graphql-yoga": "^3.5.0",
    "next": "13.1.6",
    "postcss": "latest",
		"postcss-import": "^15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "^6.5.16",
    "start-server-and-test": "^1.15.3",
		"tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx}": [
			"yarn prettier:fix",
			"yarn lint",
			"git add ."
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
  "devDependencies":{
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "cypress": "^12.5.1",
    "prettier": "^2.8.3",
    "husky": "^8.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
}
